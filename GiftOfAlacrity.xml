	<element name="Gift of Alacrity" type="Spell" source="Explorer's Guide to Wildemount" id="ID_WOTC_EGTW_SPELL_GIFT_OF_ALACRITY">
		<supports>Wizard</supports>
		<description>
			<p>You touch a willing creature. For the duration, the target can add 1d8 to its initiative rolls.</p>
		</description>
		<setters>
			<set name="keywords">dunamancy</set>
			<set name="level">1</set>
			<set name="school">divination</set>
			<set name="time">1 minute</set>
			<set name="duration">8 hours</set>
			<set name="range">Touch</set>
			<set name="hasVerbalComponent">true</set>
			<set name="hasSomaticComponent">true</set>
			<set name="hasMaterialComponent">false</set>
			<set name="materialComponent" />
			<set name="isConcentration">false</set>
			<set name="isRitual">false</set>
		</setters>
	</element>
